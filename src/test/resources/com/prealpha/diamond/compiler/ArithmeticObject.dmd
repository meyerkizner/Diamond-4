class ArithmeticObject {
	private int value;

	static ArithmeticObject new() {
		value = 0;
		return this;
	}

	int getValue() {
		return value;
	}

	void increment() {
		value += 1;
	}
}

void main() {
	ArithmeticObject obj = ArithmeticObject::new();
	for (int i = 0; i < 10; i += 1) {
		obj.increment();
	}
	int value = obj.getValue();
}
